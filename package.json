{
  "name": "elastiq",
  "version": "1.0.0",
  "description": "company website",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/*.html",
    "test": "npm run build",
    "pretest": "npm run lint",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue src",
    "lint:css": "npm run lint:css:vue && npm run lint:css:scss",
    "lint:css:vue": "stylelint 'src/**/*.vue'",
    "lint:css:scss": "stylelint 'src/**/*.scss'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meodai/elastiq.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meodai/elastiq/issues"
  },
  "homepage": "https://github.com/meodai/elastiq#readme",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "sass": "^1.26.5"
  },
  "staticFiles": {
    "staticPath": "public"
  }
}
